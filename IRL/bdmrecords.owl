     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="http://purl.org/net/irish-record-linkage/records">
        <dc:creator rdf:datatype="&xsd;string">Christophe Debruyne (KDEG, Trinity College Dublin)</dc:creator>
        <dc:contributor rdf:datatype="&xsd;string">Dolores Grant (Digital Repository of Ireland)</dc:contributor>
        <dc:title rdf:datatype="&xsd;string">Irish Record Linkage Vital Records Ontology</dc:title>
        <dc:contributor rdf:datatype="&xsd;string">Oya Beyan (Insight @ NUI Galway)</dc:contributor>
        <dc:contributor rdf:datatype="&xsd;string">PJ Mealy (Insight @ NUI Galway)</dc:contributor>
        <dc:description rdf:datatype="&xsd;string">The IRL Vital Records Ontology captures all classes and properties to create RDF encodings of the register pages and records.</dc:description>
        <owl:versionInfo rdf:datatype="&xsd;string">Version 0.3 of the lightweight ontology for describing records. Changes include:

- fixed typos in class names;
- fixed missing range for page.</owl:versionInfo>
        <owl:versionIRI rdf:resource="http://purl.org/net/irish-record-linkage/records/0.2"/>
    </owl:Ontology>
    


