    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/net/irish-record-linkage/records#recordOf -->

    <owl:ObjectProperty rdf:about="&records;recordOf">
        <rdfs:label rdf:datatype="&xsd;string">record of</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Relation between register pages and records. Register pages can have records. Records should belong to exactly one register page.</rdfs:comment>
        <rdfs:domain rdf:resource="&records;Record"/>
        <rdfs:range rdf:resource="&records;RegisterPage"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/irish-record-linkage/records#withRecord -->

    <owl:ObjectProperty rdf:about="&records;withRecord">
        <rdfs:label rdf:datatype="&xsd;string">with record</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Relation between records and register pages. Register pages can have records. Records should belong to exactly one register page.</rdfs:comment>
        <rdfs:range rdf:resource="&records;Record"/>
        <rdfs:domain rdf:resource="&records;RegisterPage"/>
        <owl:inverseOf rdf:resource="&records;recordOf"/>
    </owl:ObjectProperty>
    


