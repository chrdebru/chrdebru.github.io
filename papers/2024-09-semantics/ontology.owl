    <!-- http://www.cytomine.be/ont#annotationIsOnImage -->


    <owl:ObjectProperty rdf:about="http://www.cytomine.be/ont#annotationIsOnImage">
        <owl:inverseOf rdf:resource="http://www.cytomine.be/ont#imageHasAnnotation"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
        <rdfs:range rdf:resource="http://www.cytomine.be/ont#Image"/>
        <rdfs:comment xml:lang="fr">Lie une annotation à l&apos;image à laquelle elle est associée, permettant une relation directe qui facilite la récupération et l&apos;analyse des données centrées sur l&apos;image.</rdfs:comment>
        <rdfs:comment xml:lang="en">Links an annotation to the image it is associated with, allowing for a direct relationship that facilitates image-centric data retrieval and analysis.</rdfs:comment>
        <rdfs:label xml:lang="en">annotation is on image</rdfs:label>
        <rdfs:label xml:lang="fr">l&apos;annotation est sur l&apos;image</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.cytomine.be/ont#annotationPrecededBy -->


    <owl:ObjectProperty rdf:about="http://www.cytomine.be/ont#annotationPrecededBy">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
        <rdfs:comment xml:lang="fr">lie une annotation à l&apos;annotation précédente faite par le même utilisateur sur la même image</rdfs:comment>
        <rdfs:comment xml:lang="en">links an annotation to the previous annotation made by the same user on the same image</rdfs:comment>
        <rdfs:label xml:lang="en">annotation preceded by</rdfs:label>
        <rdfs:label xml:lang="fr">annotation précédée de</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.cytomine.be/ont#annotationTag -->


    <owl:ObjectProperty rdf:about="http://www.cytomine.be/ont#annotationTag">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
        <rdfs:range rdf:resource="http://www.cytomine.be/ont#Tag"/>
        <rdfs:comment xml:lang="en">Associates a tag with an annotation, enabling the categorization and quick retrieval of annotations based on thematic or descriptive tags.</rdfs:comment>
        <rdfs:comment xml:lang="fr">Associe un tag à une annotation, permettant la catégorisation et la récupération rapide des annotations basées sur des tags thématiques ou descriptifs.</rdfs:comment>
        <rdfs:label xml:lang="en">annotation tag</rdfs:label>
        <rdfs:label xml:lang="fr">tag d&apos;annotation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.cytomine.be/ont#hasArea -->


    <owl:ObjectProperty rdf:about="http://www.cytomine.be/ont#hasArea">
        <owl:inverseOf rdf:resource="http://www.cytomine.be/ont#isAreaOf"/>
        <rdfs:domain rdf:resource="http://www.opengis.net/ont/geosparql#Geometry"/>
        <rdfs:range rdf:resource="http://www.cytomine.be/ont#Area"/>
        <rdfs:comment xml:lang="en">Relates a geometric entity to its area, quantifying the spatial extent within a two-dimensional plane, essential for spatial analysis.</rdfs:comment>
        <rdfs:comment xml:lang="fr">Relie une entité géométrique à sa surface, quantifiant l&apos;étendue spatiale dans un plan bidimensionnel, essentiel pour l&apos;analyse spatiale.</rdfs:comment>
        <rdfs:label xml:lang="fr">a une surface</rdfs:label>
        <rdfs:label xml:lang="en">has area</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.cytomine.be/ont#hasCoordinate -->


    <owl:ObjectProperty rdf:about="http://www.cytomine.be/ont#hasCoordinate">
        <owl:inverseOf rdf:resource="http://www.cytomine.be/ont#isCoordinateOf"/>
