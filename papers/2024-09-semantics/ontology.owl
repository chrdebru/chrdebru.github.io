        <rdfs:range rdf:resource="http://www.cytomine.be/ont#Tag"/>
        <rdfs:comment xml:lang="en">Relates a user to the tags they have applied, highlighting their contribution to the categorization of annotations or images.</rdfs:comment>
        <rdfs:comment xml:lang="fr">Relie un utilisateur aux tags qu&apos;il a appliqués, mettant en évidence sa contribution à la catégorisation des annotations ou des images.</rdfs:comment>
        <rdfs:label xml:lang="fr">a appliqué le tag</rdfs:label>
        <rdfs:label xml:lang="en">has done tag</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.cytomine.be/ont#hasPerimeter -->


    <owl:ObjectProperty rdf:about="http://www.cytomine.be/ont#hasPerimeter">
